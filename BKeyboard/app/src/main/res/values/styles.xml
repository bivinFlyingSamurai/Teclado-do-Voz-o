<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SplashTheme" parent="Theme.AppCompat.Light.NoActionBar"/>

    <style name="AppTheme.Material" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="android:statusBarColor">@color/black</item>
        <item name="android:navigationBarColor">@color/black</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>

    <style name="RoundedImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerRadius">20dp</item>
    </style>

    <style name="Widget.App.Switch" parent="Widget.AppCompat.CompoundButton.Switch">

    </style>


    <style name="ColorSwitchStyle">
        <item name="colorControlActivated">@color/highlight_color_lxx_light</item>
        <item name="colorSwitchThumbNormal">@color/switchTrack</item>
        <item name="android:colorForeground">#42221f1f</item>
    </style>


</resources>